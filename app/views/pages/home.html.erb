<h1>Places</h1>
<table>
  <thead>
    <tr>
      <th>Name </th>
      <th colspan="3"></th>
    </tr>
  </thead><tbody>
    <% @places.each do |place| %>
      <tr>
        <td><%= link_to place.name, place %></td>
