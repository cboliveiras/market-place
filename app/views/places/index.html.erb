<div class="container">
<h1>Places</h1>
<table>
  <thead>
    <tr>
      <th colspan="3"></th
    </tr>
  </thead><tbody>
    <% @places.each do |place| %>
      <tr>
        <td><%= link_to place.name, place %></td>
      </tr>
    <% end %>
  </tbody>
  </table>
<br>
</div>
<div class="container">
<%= link_to 'Add a New Place', new_place_path, class: 'btn btn-danger' %><br><br>
</div>
